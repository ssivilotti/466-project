   "source": [
    "f2 = open('microgreen_id_rna.fasta', 'w')\n",
    "with open('microgreen_id_dna.fasta', 'r') as f:\n",
    "    line = f.readline()\n",
    "    while line:\n",
    "        if (line[0] == '>'):\n",
    "            f2.write(line)\n",
    "        else:\n",
    "            f2.write(line.replace('T','U'))\n",
    "        line = f.readline()\n",
    "f2.close()"
   ]
  }
